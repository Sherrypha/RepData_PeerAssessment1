library(knitr)
opts_chunk$set(echo=TRUE)
data<-read.csv(unz("activity.zip","activity.csv"), header=T,sep=",")
head(data)
data$date<-as.Date(data$date)
#histdata<-data[!is.na(data$steps),]
#with(histdata, hist(steps,date))
#head(histdata)
#aggregate number of steps per day
ag_data<-setNames(aggregate(data$steps, by=list(data$date), sum, na.omit=T),c("Date","Steps"))
#set na to 0 for days with no steps taken
ag_data[is.na(ag_data)]<-0
#A histogram of total number of steps taken each day
ggplot(ag_data,aes(Date, Steps))+ geom_histogram(stat = "identity")
#calculate mean and median
stepsmean<-mean(ag_data[,2])
stepsmedian<-median(ag_data[,2])
